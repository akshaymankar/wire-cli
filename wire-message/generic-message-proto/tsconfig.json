{
  "compilerOptions": {
    "declaration": true,
    "lib": ["DOM", "ES6"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "outDir": "web",
    "removeComments": true,
    "rootDir": "web",
    "sourceMap": false,
    "strict": true,
    "target": "ES5"
  },
  "exclude": ["node_modules"]
}
